{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "_comment": "Environment variables should be set in Vercel dashboard under Project settings > Environment Variables",
  "env": {
    "_VITE_SUPABASE_URL": "Set this in Vercel dashboard",
    "_VITE_SUPABASE_ANON_KEY": "Set this in Vercel dashboard",
    "_VITE_OPENWEATHERMAP_API_KEY": "Set this in Vercel dashboard"
  }
}

