{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production npm run check-init-issues && vite build --mode production && node scripts/check-bundle-reference-errors.js",
    "build:debug": "cross-env NODE_ENV=development npm run check-init-issues && vite build --mode debug",
    "build:dev": "cross-env NODE_ENV=development npm run check-init-issues && vite build --mode development",
    "netlify-build": "npm ci --legacy-peer-deps && npm run build",
    "analyze": "npm run check-init-issues && vite build --mode production && npx rollup-plugin-visualizer --template treemap --open dist/stats.html",
    "test": "vitest run --coverage=false",
    "test:bg-manager": "vitest run tests/backgroundManager.test.tsx",
    "lint": "eslint . --max-warnings=1000",
    "lint:strict": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "check-init-issues": "node scripts/check-init-issues.js",
    "preview": "vite preview",
    "preview:ci": "vite preview --host 0.0.0.0 --port 4174 --logLevel info",
    "test-preview": "node scripts/test-preview.js",
    "secret-scan": "node scripts/secret-scan.js",
    "secret-scan:pre-commit": "node scripts/secret-scan.js --staged",
    "secret-scan:ci": "node scripts/secret-scan.js --ci",
    "lhci": "lhci autorun",
    "lhci:collect": "lhci collect",
    "lhci:assert": "lhci assert",
    "lhci:upload": "lhci upload",
    "security:check": "npm run secret-scan && npm run lint",
    "security:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "prebuild": "npm run type-check",
    "prebuild:with-lint": "npm run type-check && npm run lint",
    "prebuild:local": "npm run check-init-issues && npm run type-check && npm run lint",
    "precommit": "lint-staged"
  },
